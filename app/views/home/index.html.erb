
<% unless session[:user_id] %>
  <%= link_to "Sign in Dev", "/auth/developer", class: "btn btn-default" %>
  <%= link_to "Sign in with Instagram", "/auth/instagram", class:
  "btn btn-default" %>
<% end %>


<% @sub_array.each do |tweet| %>
  <%= tweet.user.username %>
  <%= tweet.text %>
  <%= tweet.as_json["created_at"] %>
  <% tweet.media.as_json.each do |media| %>
    <%= image_tag(media["media_url_https"], height: '300' ) %>
  <% end %>
  <br>###<br>
<% end %>
