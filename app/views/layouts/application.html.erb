<!DOCTYPE html>
<html>
<head>
  <title>C3ProjectsSeemore</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <header class="nav navbar-default">
    <h2>FriendFace</h2>
    <%= form_for :instagram, url: results_path do |f| %>
      <%= f.label "Search for an Instagram feed" %>
      <%= f.text_field :query %>

      <%= f.submit "Find something interesting!" %>
    <% end %>
  </header>

<div class="container">
  <%= yield %>
</div>

</body>
</html>
