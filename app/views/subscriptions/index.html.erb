<h1 class="center"><%= @current_user.name.capitalize %>'s Subscriptions </h1>
<ul class="search-results">
<% @subscriptions.each do |s| %>
    <li><%= link_to image_tag(s.avatar_url, class: "img-thumbnail", size: "50x50") %>
    <%= link_to s.handle %>
    <% if s.source == "instagram" %>
      <%= link_to image_tag("app/assets/images/insta.png", class: "img-thumbnail", size: "20x20") %>
    <% else %>
      <%= link_to image_tag("app/assets/images/tweet.png", class: "img-thumbnail", size: "20x20") %>
    <% end %>
  <%= link_to "Unsubscribe" %></li>
<% end %>
</ul>
