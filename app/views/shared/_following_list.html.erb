<table class="table">
  <% all_prey.each do |prey| %>
    <tr>
      <td>
        <%= image_tag prey.photo_url, class: "media-object" %>
      </td>
      <td>
        <h4><%= link_to prey.username, prey.profile_url, class: "media-heading" %></h4>
      </td>
      <td>
        <%= button_to "Unfollow", unfollow_path, class: "btn btn-danger btn-sm",
          method: :patch,
          params: {
            uid: prey.uid,
            provider: prey.provider
          }
        %>
      </td>
    </tr>
  <% end %>
</table>
