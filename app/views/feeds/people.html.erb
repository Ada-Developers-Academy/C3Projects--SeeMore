<!-- List of all people user follows with options to delete and add more -->
<h2>Assassinate</h2>

<% unless session[:user_id].nil? %>
  <h3>Targets</h3>
  <% @people.each do |person| %>
  <p>
    <%= person.username %>
    <%= link_to "X", instagram_path(id: person.id), method: :delete, class: "btn btn-danger" %>
  </p>
  <% end %>
<% end  %>
