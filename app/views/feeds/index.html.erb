<% @posts.each do |post| %>
  <% if post.class == Tweet %>
    <%= raw tweet_oembed(post) %>
  <% elsif post.class == Gram %>
    <% response = HTTParty.get("http://api.instagram.com/oembed?url=http://instagr.am/#{post.link[22,20]}") %>
    <%= raw response["html"] %>
  <% end %>
<% end %>





