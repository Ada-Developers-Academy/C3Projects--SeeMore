<h1>I can haz feedz?</h1>

<%= link_to "Dev Login", login_path(provider: :developer), class: "btn btn-primary" %>
<%= link_to "Login with Instagram", login_path(provider: :instagram), class: "btn btn-primary" %>
<%= link_to "Logout", logout_path, method: :delete, class: "btn btn-danger" %>
<% unless @user.nil?  %>
  <%= @user.username %>
<% end %>

<%= form_tag(instagrams_path, :method => "get", class: "navbar-form", id: "search-form") do %>
    <div class="input-append">
      <%= text_field_tag :search, params[:search], class: "span2", placeholder: "Search Users" %>
      <button class="btn" type="submit"><i class="icon-search">Search</i></button>
    </div>
<% end %>
